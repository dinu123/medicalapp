{
  "name": "medical-app-deployment",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "npm run dev",
    "dev:backend": "cd backend && npm start",
    "build": "npm run build",
    "start": "npm run dev",
    "install:all": "npm install && cd backend && npm install"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}